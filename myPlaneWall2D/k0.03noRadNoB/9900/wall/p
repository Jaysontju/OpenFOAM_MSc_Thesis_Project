FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "9900/wall";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 100000;

boundaryField
{
    leftLet
    {
        type            calculated;
        value           uniform 0;
    }
    rightLet
    {
        type            calculated;
        value           uniform 0;
    }
    frontAndBack
    {
        type            empty;
    }
    wall_to_bottomAir
    {
        type            calculated;
        value           uniform 0;
    }
    wall_to_topAir
    {
        type            calculated;
        value           uniform 0;
    }
}


